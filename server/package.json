{
  "name": "bygagoos-server",
  "version": "2.0.0",
  "description": "Backend API for ByGagoos - Gestion Textile",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:verbose": "nodemon --verbose server.js",
    "production": "cross-env NODE_ENV=production node server.js",
    "test": "node test-db.js",
    "init-db": "node init-db.js",
    "create-user": "node create-test-user.js",
    "migrate": "node migrate-commandes.js",
    "debug": "node --inspect server.js",
    "build": "echo 'No build needed for server'",
    "vercel-build": "echo 'Skipping server build for Vercel'"
  },
  "keywords": [
    "bygagoos",
    "textile",
    "management",
    "api",
    "postgresql",
    "vercel"
  ],
  "author": "ByGagoos Team",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^18.0.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}