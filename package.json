{
  "name": "bygagoos-avant-push",
  "private": true,
  "version": "2.0.0",
  "type": "commonjs",
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev:all": "concurrently \"npm --prefix client run dev\" \"npm --prefix server run dev\"",
    "dev": "npm run dev:all",
    "build": "npm run build:all",
    "build:all": "npm run build:client && npm run build:server",
    "build:client": "npm --prefix client run build",
    "build:server": "npm --prefix server run build",
    "install:all": "npm install && cd client && npm install && cd ../server && npm install",
    "start": "npm --prefix server start",
    "start:client": "npm --prefix client start",
    "start:server": "npm --prefix server start",
    "test": "echo \"No tests configured at root\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeMizoo/gagoos.git"
  },
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}